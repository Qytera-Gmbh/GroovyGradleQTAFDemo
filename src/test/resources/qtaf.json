{
  "tests": {
    "suite": {
      "name": "TestQytera"
    },
    "package":"de.qytera.ggqd.tests",
    "continueOnAssertionFailure": true
  },
  "driver": {
    "name": "chrome",
    "version": "118",
    "remoteUrl": "https://www.qytera.de",
    "implicitWaitTimeout": 30,
    "quitAfterTesting": true,
    "headless": false,
    "screenshots": {
      "beforeScenario": true,
      "afterScenario": true,
      "beforeStep": true,
      "afterStep": true,
      "afterStepFailure": true
    }
  },
  "logging": {
    "enabled": true
  },
  "htmlReport": {
    "enabled": true
  },
  "framework": {
    "packageNames": ["de.qytera.ggqd"]
  },
  "console": {
    "colors": true
  }
}