{
  "tests": {
    "suite": {
      "name": "TestQytera"
    },
    "package": "de.qytera.ggqd.tests",
    "continueOnAssertionFailure": false
  },
  "driver": {
    "name": "chrome",
    "version": "118",
    "remoteUrl": "https://www.qytera.de",
    "implicitWaitTimeout": 30,
    "quitAfterTesting": true,
    "headless": false,
    "screenshots": {
      "beforeScenario": true,
      "afterScenario": true,
      "beforeStep": true,
      "afterStep": true,
      "afterStepFailure": true
    }
  },
  "logging": {
    "enabled": true
  },
  "htmlReport": {
    "enabled": true
  },
  "framework": {
    "packageNames": [
      "de.qytera.ggqd"
    ]
  },
  "console": {
    "colors": true
  },
  "xray": {
    "enabled": true,
    "projectKey": "QTAF",
    "service": "cloud",
    "authentication": {
      "xray": {
        "clientId": "...",
        "clientSecret": "...",
        "bearerToken": null
      },
      "jira": {
        "username": null,
        "apiToken": null
      }
    },
    "resultsUpload": {
      "scenarioReportEvidence": false,
      "scenarioImageEvidence": false,
      "testPlanKey": null,
      "environments": {
        "enabled": true,
        "os": false,
        "driver": true
      },
      "customStatus": {
        "test": {
          "passed": null,
          "failed": null,
          "pending": null,
          "skipped": null
        },
        "step": {
          "passed": null,
          "failed": null,
          "pending": null,
          "skipped": null,
          "undefined": null
        }
      },
      "tests": {
        "info": {
          "keepJiraSummary": false,
          "steps": {
            "update": true,
            "merge": true
          }
        },
        "iterations": {
          "parameters": {
            "maxLength": {
              "name": null,
              "value": 100
            }
          }
        }
      }
    }
  }
}